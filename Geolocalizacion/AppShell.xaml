<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Geolocalizacion.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Geolocalizacion"
    Shell.FlyoutBehavior="Flyout"
    Title="Geolocalizacion">

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:Views.vLogin}"
        Route="LoginPage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:Views.vHome}"
        Route="HomePage" />


</Shell>
