<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Geolocalizacion.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Geolocalizacion"
    xmlns:views="clr-namespace:Geolocalizacion.Views"
     Shell.FlyoutBehavior="Disabled"
    Title="Geolocalizacion">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:vHome}"
        Route="HomePage" />

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate views:vLogin}"
        Route="LoginPage" />

</Shell>
